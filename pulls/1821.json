      "repos_url": "https://api.github.com/users/gzJx0DuTRHytnHe7P5RmMbPf3wKy2BztweVGXTf/repos",
      "events_url": "https://api.github.com/users/gzJx0DuTRHytnHe7P5RmMbPf3wKy2BztweVGXTf/events%7B/privacy%7D",
