      "html_url": "https://github.com/doitian",
      "followers_url": "https://api.github.com/users/doitian/followers",
      "following_url": "https://api.github.com/users/doitian/following%7B/other_user%7D",
      "gists_url": "https://api.github.com/users/doitian/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/doitian/starred%7B/owner%7D%7B/repo%7D",
